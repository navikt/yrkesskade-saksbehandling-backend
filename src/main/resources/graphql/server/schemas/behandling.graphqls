scalar LocalDate
scalar Instant

type Sak {
    sakId: ID!
    tema: String
    brukerIdentifikator: String!
    sakstype: String
    saksstatus: String
}

type Behandling {
    behandlingId: ID!
    behandlingsfrist: Instant
    opprettetTidspunkt: Instant
    status: String
    sak: Sak
}

input Page {
    size: Int
    page: Int
}

type Query {
    hentBehandlinger(page: Page!): [Behandling]!
    hentEgneBehandlinger(page: Page!): [Behandling]!
    antallBehandlinger: Int!
}